@using Shopping.Enums
@using Shopping.Models.ViewModels
@model Vm_LoginRegister

@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="https://static.mutualart.com/bundles/css?v=3TEyfaa4aFSkqvOFXroAqJmBFQsT0-r-Z8ZVWBkW2h01" type="text/css"
          rel="stylesheet">

    <style>
        .story-container {
            margin-top: 24px;
            margin-bottom: 10px;
            display: flex;
            flex-flow: wrap;
        }

        .story {
            margin-bottom: 32px;
            width: 50%;
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            -webkit-box-pack: start;
            justify-content: start;
        }

            .story h4 {
                margin: 0px 0px 0.25rem;
                font-size: 16px;
                font-weight: 700;
                line-height: 20px;
                color: rgb(39, 40, 41);
                text-transform: none;
            }

            .story p {
                margin: 0px;
                line-height: 20px;
                font-weight: 400;
                color: rgb(90, 94, 102);
                font-size: 14px;
                text-transform: none;
            }
    </style>
</head>

<body class="new-nav not-past-paid-user sub-1- gray_background" style="padding-top:60px">
    <div class="page-content SignUp">
        <div class="valign-boxes" id="">
            <div class="container-fluid ">
                <div class="row columns-container margin-top-50">
                    <div class="col-md-6">

                        <script>
                            $(function () {
                                $("#email").focus()
