                        </script>


                        <div class="signup-box">
                            <div class="modal-body">
                                <form class="sign-up-form" id="signupForm">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h1 class="signup_header p-0">
                                                Login to your supplier panel
                                            </h1>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <a class="btn btn-2 google-plus-btn social-btn-text" href="@Url.Action("GoogleLogin", "Account", new { role = UserRole.Supplier, returnUrl = Model.ReturnUrl })" onclick="magoo.checkStatus()" style="background-color: #fff; color: black; border-color: #eee; box-shadow: 0 0 4px 0 #eee; border-radius: 3px;">
                                            <img src="/images/google_logo.png" style=" height: 60%; margin-bottom: 5px; margin-right: 5px; ">
                                            Sign up with Google
                                        </a>
                                    </div>
                                    <div class="hr-lines">or</div>
                                    <div class="form-group"> <input tabindex="1" type="email" name="email" id="email" class="default high" placeholder="Email" required=""> </div>
                                    <div class="form-group"> <input tabindex="2" type="password" minlength="7" name="userPassword" class="default high" autocomplete="new-password" id="txtSignupPassword" placeholder="Password" required=""> </div>
                                    <div class="row">
                                        <div class="col-xs-6">
                                            <div class="form-group"> <input tabindex="3" type="text" name="userFirstName" class="default high" placeholder="First Name" required="" maxlength="50"> </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="form-group"> <input tabindex="4" type="text" name="userLastName" class="default high" placeholder="Last Name" required="" maxlength="50"> </div>
                                        </div>
                                    </div>

                                    <div class="g-recaptcha col-xs-12 no-padding-left margin-bottom-15" data-size="invisible" data-sitekey="6LfV714UAAAAACeEAaHCnhTf-mUwBW2MjsOvBkkk" data-callback="sendRegistration"></div>

                                    <div class="form-messages"></div>

                                    <div class="form-group"> <div type="submit" class="btn btn-1" onclick="validateRegistration()" tabindex="8" style="background-color:rgb(130, 106, 251)">Sign Up</div> </div>
                                    <p class="buttons-description">By clicking, you accept the <a target="_blank" href="/Static/TermOfUse">Terms of Service</a> and confirm that you have read the <a target="_blank" href="/Static/PrivacyPolicy">Privacy Policy</a>.</p>


                                    <p class="par-with-cta" id="login">Already registered? <a class="cta" href="/login/?">Log In</a></p>

                                    <input type="hidden" id="planId" name="planId" value="">
                                </form>
                            </div>
